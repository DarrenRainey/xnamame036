 <script type="text/javascript">
  $(document).ready(function(){
       $('#_pacman').width(200);
       $('#_pacman').mouseover(function()
       {
          $(this).css("cursor","pointer");
       });
       $("#_pacman").toggle(function()
          {$(this).animate({width: "500px"}, 'slow');}, 
          function()
          {$(this).animate({width: "200px"}, 'slow');
       });
       $('#_invaders').width(200);
       $('#_invaders').mouseover(function()
       {
          $(this).css("cursor","pointer");
       });
       $("#_invaders").toggle(function()
          {$(this).animate({width: "500px"}, 'slow');}, 
          function()
          {$(this).animate({width: "200px"}, 'slow');
       });
    });
    </script>
